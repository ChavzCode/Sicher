<section class="container min-link">
    <div class="section">
        {{#if token}}
            <div class="token-box black-api-box">
                <div class="token-container">
                    <p>Downloading From:</p>
                    <p id="downloaded-token">{{token}}</p>    
                </div>
            </div>
        {{else}}
            <div class="token-box black-api-box">
                <div class="token-container">
                    <p>No token obtain</p>
                </div>
            </div>
        {{/if}}
    </div>

    
    <div class="link-list">
        {{!-- Content Generated by API --}}
    </div>

</section>

<section class="container">
    <div class="advanced-features">
        <div class="feature feature-cancel">
            <a class="import-cancel" href="/api">Cancel</a>
        </div>

        <div class="ctn-selection">
            <div class="legend">
                <p>Selected</p> <div class="selected-legend"></div>    
            </div>
            <div class="legend">
                <p>Unselected</p> <div class="unselected-legend"></div>
            </div>
        </div>

        <div class="feature feature-done">
            <a class="import-done">Done</a>
        </div>
    </div>
</section>